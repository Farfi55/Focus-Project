<?xml version="1.0" encoding="UTF-8"?>

<?import io.github.palexdev.materialfx.controls.MFXScrollPane?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane id="infoPageRoot" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mafiadelprimobanco.focusproject.controller.InfoPageController">
   <children>
      <MFXScrollPane fitToWidth="true" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <content>
            <VBox prefHeight="200.0" prefWidth="100.0" spacing="15.0">
               <children>
                  <HBox maxHeight="1.7976931348623157E308" spacing="10.0">
                     <children>
                        <VBox fx:id="imageContainer" prefHeight="200.0" prefWidth="100.0" spacing="10.0">
                           <children>
                              <ImageView fx:id="homeImage" fitHeight="300.0" fitWidth="400.0" pickOnBounds="true" preserveRatio="true" />
                              <ImageView fx:id="progressImage" fitHeight="300.0" fitWidth="400.0" pickOnBounds="true" preserveRatio="true" />
                           </children>
                           <HBox.margin>
                              <Insets />
                           </HBox.margin>
                        </VBox>
                        <Label fx:id="helpLabel" maxWidth="1.7976931348623157E308" minHeight="-Infinity" text="help label" wrapText="true" />
                     </children>
                  </HBox>
                  <HBox prefHeight="100.0" prefWidth="200.0" spacing="15.0">
                     <children>
                        <Label fx:id="madeByLabel" alignment="CENTER" maxWidth="1.7976931348623157E308" text="Label">
                           <HBox.margin>
                              <Insets top="3.0" />
                           </HBox.margin></Label>
                        <Hyperlink fx:id="githubLink" text="Project on GitHub" />
                     </children>
                  </HBox>
               </children>
               <padding>
                  <Insets bottom="15.0" left="15.0" right="15.0" top="15.0" />
               </padding>
            </VBox>
         </content>
      </MFXScrollPane>
   </children>
</AnchorPane>
